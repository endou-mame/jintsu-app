{
  "mcpServers": {
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp@latest"
      ],
      "env": {}
    },
    "markitdown": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "markitdown-mcp"
      ],
      "env": {}
    },
    "textlint": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "textlint",
        "--mcp",
        "--config",
        "/home/kakiuchi-shigenao/.textlintrc"
      ],
      "env": {}
    }
  }
}